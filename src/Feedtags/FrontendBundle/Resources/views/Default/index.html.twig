{% extends 'FeedtagsFrontendBundle::layout.html.twig' %}
{% block ng_app %}feedtagsApp{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts
    'bundles/feedtagsfrontend/js/*.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

{% block page_content %}
    <h1 class="page-header">Welcome</h1>
    {% if is_granted('ROLE_USER') %}
        <div ng-view>
            Loading...
        </div>
    {% else %}
        {% include "FeedtagsUserBundle:Security:login_content.html.twig" %}
    {% endif %}
{% endblock %}
